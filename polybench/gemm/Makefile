EXAMPLE_NAME := gemm
SRC := gemm.c
LDFLAGS := -lm -ldl

# Size configurations - can be overridden via command line
# SMALL: 128x128x128, MEDIUM: 1024x1024x1024, LARGE: 2000x2000x2000, EXTRALARGE: 4000x4000x4000
SMALL_CFLAGS ?= -DNI=128 -DNJ=128 -DNK=128
MEDIUM_CFLAGS ?= -DNI=1024 -DNJ=1024 -DNK=1024
LARGE_CFLAGS ?= -DNI=2000 -DNJ=2000 -DNK=2000
EXTRALARGE_CFLAGS ?= -DNI=5000 -DNJ=5000 -DNK=5000

include ../carts.mk
