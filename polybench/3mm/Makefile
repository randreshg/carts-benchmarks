EXAMPLE_NAME := 3mm
SRC := 3mm.c
LDFLAGS := -lm -ldl

# Size configurations - can be overridden via command line
# LARGE (64-core): 3072 (48x64). EXTRALARGE (1920-worker): 5760 (3x1920). 6.6x LARGE
SMALL_CFLAGS ?= -DSMALL_DATASET
MEDIUM_CFLAGS ?= -DSTANDARD_DATASET
LARGE_CFLAGS ?= -DNI=3072 -DNJ=3072 -DNK=3072 -DNL=3072 -DNM=3072
EXTRALARGE_CFLAGS ?= -DNI=5760 -DNJ=5760 -DNK=5760 -DNL=5760 -DNM=5760

include ../carts.mk
